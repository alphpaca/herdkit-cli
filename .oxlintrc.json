{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "plugins": ["typescript", "react"],
  "categories": {
    "correctness": "error",
    "suspicious": "warn",
    "perf": "warn"
  },
  "rules": {},
  "settings": {
    "react": {
      "formComponents": [],
      "linkComponents": [],
      "version": "19",
      "componentWrapperFunctions": []
    }
  },
  "env": {
    "builtin": true
  },
  "globals": {},
  "ignorePatterns": ["dist", "node_modules"]
}
